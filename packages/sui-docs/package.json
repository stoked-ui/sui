{
  "name": "@stoked-ui/docs",
  "version": "0.1.0-alpha.9",
  "private": false,
  "author": "SUI Team",
  "description": "SUI Docs - Documentation building blocks.",
  "main": "./src/index.ts",
  "keywords": [
    "react",
    "react-component",
    "stoked ui",
    "material ui",
    "material design",
    "docs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stoked-ui/mono.git",
    "directory": "packages/sui-docs"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stoked-ui/mono/issues"
  },
  "homepage": "https://github.com/stoked-ui/mono/tree/master/packages/sui-docs",
  "scripts": {
    "build": "pnpm build:legacy && pnpm build:modern && pnpm build:node && pnpm build:stable && pnpm build:types && pnpm build:copy-files",
    "build:legacy": "node ../../scripts/build.mjs legacy",
    "build:modern": "echo 'Skip modern build'",
    "build:node": "node ../../scripts/build.mjs node",
    "build:stable": "node ../../scripts/build.mjs stable",
    "build:types": "tsc --emitDeclarationOnly",
    "build:copy-files": "node ../../scripts/copyFiles.mjs ./src/translations/translations.json:./translations/translations.json ./src/translations/translations.json:./node/translations/translations.json",
    "prebuild": "rimraf build",
    "release": "pnpm build && pnpm publish",
    "test": "exit 0"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.9",
    "clsx": "^2.1.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@stoked-ui/editor": "workspace:^",
    "@stoked-ui/file-explorer": "workspace:^",
    "@stoked-ui/media-selector": "workspace:^",
    "@stoked-ui/timeline": "workspace:^",
    "@mui/base": "^5.0.0-beta.40",
    "@mui/system": "^5.16.0",
    "@mui/styled-engine": "^5.15.14",
    "@mui/utils": "^5.16.0",
    "@types/node": "^18.19.25",
    "@types/prop-types": "^15.7.11",
    "@types/react": "^18.3.1",
    "next": "^13.5.1",
    "react": "18.3.1",
    "typescript": "^4.9.5",
    "@types/nprogress": "^0.2.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.15.21",
    "@mui/material": "^5.15.21",
    "@stoked-ui/docs-utils": "workspace:^",
    "@stoked-ui/editor": "workspace:^",
    "@stoked-ui/file-explorer": "workspace:^",
    "@stoked-ui/media-selector": "workspace:^",
    "@stoked-ui/timeline": "workspace:^",
    "@types/react": "^18.3.1",
    "next": "^13.5.1 || ^14",
    "react": "18.3.1"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
