{
  "name": "@stoked-ui/common",
  "version": "0.1.1",
  "description": "Common utilities and components for Stoked UI",
  "author": "Brian Stoker",
  "main": "src/index.ts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stoked-ui/common/issues"
  },
  "homepage": "https://common.stoked-ui.com",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "keywords": [
    "react",
    "react-component",
    "sui",
    "mui",
    "stoked ui",
    "material ui",
    "material design"
  ],
  "scripts": {
    "typescript": "tsc -p tsconfig.json",
    "build": "pnpm build:modern && pnpm build:node && pnpm build:stable && pnpm build:types && pnpm build:copy-files ",
    "build:modern": "node ../../scripts/build.mjs modern",
    "build:node": "node ../../scripts/build.mjs node",
    "build:stable": "node ../../scripts/build.mjs stable",
    "build:copy-files": "node ../../scripts/copyFiles.mjs",
    "build:types": "node ../../scripts/buildTypes.mjs",
    "prebuild": "rimraf build tsconfig.build.tsbuildinfo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stoked-ui/sui.git",
    "directory": "packages/sui-common"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.7",
    "@mui/base": "^5.0.0-beta.40",
    "@mui/system": "^5.15.20",
    "@mui/utils": "^5.15.20",
    "@tempfix/idb": "^8.0.3",
    "clsx": "^2.1.1",
    "framer-motion": "^12.4.10",
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.15.21",
    "@mui/material": "^5.15.21",
    "react": "18.3.1",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@stoked-ui/internal-test-utils": "workspace:^",
    "@types/chai": "^4.3.17",
    "@types/prop-types": "^15.7.12",
    "@types/react-transition-group": "^4.4.10",
    "chai": "^4.4.1",
    "jest": "^29.7.0",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.2.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
