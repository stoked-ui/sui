{
  "name": "@stoked-ui/media-selector",
  "author": "Brian Stoker",
  "private": false,
  "version": "0.1.4-a.0",
  "main": "src/index.ts",
  "typings": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "build",
    "next.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stoked-ui/sui",
    "directory": "packages/sui-media-selector"
  },
  "publishConfig": {
    "access": "public",
    "directory": "build",
    "main": "index.js",
    "module": "index.modern.js",
    "typings": "index.d.ts",
    "types": "index.d.ts"
  },
  "sideEffects": false,
  "scripts": {
    "build": "pnpm build:all",
    "typescript": "pnpm build",
    "copyToDocs": "cp -r ./dist/assets ../../docs/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "build:all": "pnpm build:modern && pnpm build:node && pnpm build:stable && pnpm build:types && pnpm build:copy-files && pnpm copy:nextconfig",
    "build:modern": "node ../../scripts/build.mjs modern",
    "build:node": "node ../../scripts/build.mjs node",
    "build:stable": "node ../../scripts/build.mjs stable",
    "build:copy-files": "node ../../scripts/copyFiles.mjs",
    "build:types": "node ../../scripts/buildTypes.mjs",
    "copy:nextconfig": "cp ./next.config.js ./build/",
    "prebuild": "rimraf build tsconfig.build.tsbuildinfo",
    "release": "pnpm build && pnpm publish"
  },
  "peerDependencies": {
    "@stoked-ui/common": "workspace:^",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.24.7",
    "@stoked-ui/common": "workspace:^",
    "@types/react": "^18.3.1",
    "@types/wicg-file-system-access": "^2023.10.5",
    "eslint": "^8.57.0",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "formdata-node": "^6.0.3",
    "jszip": "^3.10.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./next.config.js": "./next.config.js"
  },
  "nextjs": {
    "transpilePackages": true
  }
}
